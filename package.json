{
  "name": "@oclif/plugin-plugins",
  "description": "plugins plugin for oclif",
  "version": "1.0.0",
  "author": "Jeff Dickey @jdxcode",
  "oclif": {
    "commands": "./lib/commands",
    "scope": "heroku-cli",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "bugs": "https://github.com/oclif/plugin-plugins/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@oclif/command": "^1.2.21",
    "chalk": "^2.3.1",
    "cli-ux": "^3.3.19",
    "debug": "^3.1.0",
    "fs-extra": "^5.0.0",
    "http-call": "^5.0.2",
    "load-json-file": "^4.0.0",
    "npm-run-path": "^2.0.2",
    "semver": "^5.5.0",
    "tslib": "^1.9.0",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "@oclif/config": "^1.3.45",
    "@oclif/dev-cli": "^0.3.14",
    "@oclif/errors": "^1.0.1",
    "@oclif/plugin-help": "^1.0.0",
    "@oclif/test": "^0.10.15",
    "@oclif/tslint": "^0.2.7",
    "@types/chai": "^4.1.2",
    "@types/fs-extra": "^5.0.0",
    "@types/load-json-file": "^2.0.7",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.5",
    "@types/semver": "^5.5.0",
    "@types/supports-color": "^3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "fancy-test": "^1.0.1",
    "globby": "^7.1.1",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/oclif/plugin-plugins",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "oclif/plugin-plugins",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"tsc -p test --noEmit\" \"tslint -p test\"",
    "postpublish": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
